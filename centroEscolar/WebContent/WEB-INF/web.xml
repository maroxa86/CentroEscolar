<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>centroEscolar</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <error-page>
    <exception-type>java.lang.RuntimeException</exception-type>
    <location>/error.jsp</location>
  </error-page>
  <servlet>
  	<description></description>
  	<display-name>ControladorAlumnos</display-name>
  	<servlet-name>ControladorAlumnos</servlet-name>
  	<servlet-class>org.centroescolar.controlador.ControladorAlumnos</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControladorAlumnos</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>